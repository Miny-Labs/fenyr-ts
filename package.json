{
    "name": "fenyr-ts",
    "version": "1.0.0",
    "description": "Multi-agent AI trading system for WEEX Exchange - GPT-5.2 powered",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "dev": "tsx watch src/index.ts",
        "start": "node dist/index.js",
        "single": "tsx src/index.ts --mode single",
        "hft": "tsx src/index.ts --mode hft",
        "team": "tsx src/index.ts --mode team",
        "lint": "eslint src/**/*.ts",
        "test": "vitest"
    },
    "keywords": [
        "trading",
        "ai",
        "agents",
        "weex",
        "cryptocurrency",
        "gpt-5.2",
        "openai"
    ],
    "author": "Miny Labs",
    "license": "MIT",
    "dependencies": {
        "openai": "^4.70.0",
        "axios": "^1.7.0",
        "zod": "^3.23.0",
        "ws": "^8.18.0",
        "commander": "^12.0.0",
        "chalk": "^5.3.0",
        "dotenv": "^16.4.0"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "@types/ws": "^8.5.0",
        "typescript": "^5.6.0",
        "tsx": "^4.19.0",
        "vitest": "^2.1.0",
        "eslint": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0"
    },
    "engines": {
        "node": ">=20.0.0"
    }
